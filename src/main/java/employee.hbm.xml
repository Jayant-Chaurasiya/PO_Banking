<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.revature.project1.model.Employee"  table = "employees">
      
      <meta attribute = "class-description">
         This class contains the employee detail. 
      </meta>
      
      <id name = "employeeId" type = "int" column = "employeeId">
         <generator class="increment"/>
      </id>
      
      <property name = "employeeName" type = "string"/>
      <property name = "employeeContact"  type = "string"/>
      <property name = "employeeEmail"  type = "string"/>
      <property name = "employeePassword"  type = "string"/>
      <property name = "employeeAddress"  type = "string"/>
     
      
   </class>
</hibernate-mapping>